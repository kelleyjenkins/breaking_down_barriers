<div class="all">
<h1 class="header_login"> Log In! </h1>
<div class="login">
  <%= form_tag do  %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
    <%= submit_tag "Sign In" %>
  <% end %>
</div>
<div>
  <%= link_to "Create Account", new_employer_path %>

</div>
