<div class="form">
  <%= form_for @profile do |f| %>
    <%= f.label :street_address %><br/>
    <%= f.text_field :street_address %><br/>
    <%= f.label :city %><br/>
    <%= f.text_field :city %><br/>
    <%= f.label :state %><br/>
    <%= f.text_field :state %><br/>
    <%= f.label :zip %><br/>
    <%= f.text_field :zip %><br/>
    <%= f.label :summary %><br/>
    <%= f.text_area :summary %><br/>
    <%= f.label :skills %><br/>
    <%= f.text_area :skills %><br/>
    <%= f.label :profile_pic %>
    <%= f.text_field :profile_pic %><br/>
    <%= f.label :mentor %>
    <%= f.select :mentor, options_for_select(Profile.mentors.map {|k, v| [k, k]}) %><br/>
    <%= f.submit %>
  <% end %>
